<h1>Usuarios</h1>

<table id="table" class="table table-striped table-no-bordered table-hover" cellspacing="20">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Email</th>
      <th>CPF</th>
      <th>CNS</th>
      <th>Data de Nascimento</th>
      <th>CEP</th>
      <th>Logradouro</th>
      <th>Complemento</th>
      <th>Cidade</th>
      <th>UF</th>
      <th>Codigo IBGE</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |object| %>
      <tr id="<%= dom_id(object) %>" >
          <td><%= object.name %></td>
          <td><%= object.email %></td>
          <td><%= object.cpf %></td>
          <td><%= object.cns %></td>
          <td><%= object.birthday.strftime("%d/%m/%Y") %></td>
          <td><%= object&.address&.zip_code %></td>
          <td><%= object&.address&.street_name %></td>
          <td><%= object&.address&.extra_info %></td>
          <td><%= object&.address&.city %></td>
          <td><%= object&.address&.state_code %></td>
          <td><%= object&.address&.ibge_code %></td>
      </tr>
    <% end %>
  </tbody>
</table>
